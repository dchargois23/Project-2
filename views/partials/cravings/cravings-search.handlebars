<div class="holy-grail-grid">
  <div class="holy-grail-header">
    <nav class="top-bar topbar-responsive">
      <div class="top-bar-title">
        <span data-responsive-toggle="topbar-responsive" data-hide-for="medium">
          <button class="menu-icon" type="button" data-toggle></button>
        </span>
        <a class="topbar-responsive-logo" href="#"><strong>{{{name}}}</strong></a>
      </div>
      <div id="topbar-responsive" class="topbar-responsive-links">
        <div class="top-bar-right">
          <ul class="menu simple vertical medium-horizontal">
            <li><a href="#">Home</a></li>
            <li><a href="#">My Favorites</a></li>
            <li>{{{member-name}}}</li>
          </ul>
        </div>
      </div>
    </nav>
  </div>

  <div id="resList" class="holy-grail-left">
    <h3>Nav</h3>
    <ul id="listbox-groups">

    </ul>

    <ul id="resMenu" class="container" style="text-align: center;"></ul>
  </div>



  <div class="holy-grail-middle">
    <h3>Content</h3>
    <div id="map" style="height:200px; width: 200px></div>
  </div>

  <h3>{{wht was searched for }}</h3>
  <ul>
    {{#each menuItem}}
    <li>
      <p>Restaurant: {{foodApp}}</p>
      <p>Food: {{foodName}}</p>
      <button>See Menu: {{}}</button>
      <button>Save to Favorites: {{}}</button>
    </li>
    {{/each}}


    <div class="holy-grail-middle">
      <h3>Content</h3>
      <div id="map" style="height:500px; width: 500px"></div>
    </div>

</div>